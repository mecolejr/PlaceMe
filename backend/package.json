{
  "name": "trueplace-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "test": "vitest run | cat",
    "prisma": "prisma",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:generate": "prisma generate",
    "seed": "tsx src/seed.ts",
    "data:phase2": "tsx src/scripts/import_phase2_stubs.ts",
    "data:stubs": "tsx src/scripts/fetch_fbi.ts && tsx src/scripts/fetch_census.ts",
    "data:import": "tsx src/scripts/import_basic_data.ts",
    "data:refresh": "pnpm data:stubs && pnpm data:import",
    "data:refresh:all": "pnpm data:refresh && pnpm data:phase2 && pnpm data:live && curl -s http://localhost:4000/api/admin/dataset | jq -r '.fingerprint' || true",
    "data:live": "tsx src/scripts/import_live.ts",
    "data:refresh:nightly": "pnpm data:refresh && pnpm data:phase2 && tsx src/scripts/fingerprint.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.16.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "prisma": "^5.16.1",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.4",
    "typescript": "^5.5.4",
    "vitest": "^2.1.9",
    "supertest": "^7.0.0",
    "@types/supertest": "^2.0.16"
  }
}
